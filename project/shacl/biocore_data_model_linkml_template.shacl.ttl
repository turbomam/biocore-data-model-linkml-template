@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix biocore_data_model_linkml_template: <https://w3id.org/turbomam/biocore-data-model-linkml-template/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:description ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ] ;
    sh:targetClass schema:Thing .

biocore_data_model_linkml_template:PlaceholderCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A holder for Placeholder objects" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class biocore_data_model_linkml_template:Placeholder ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path biocore_data_model_linkml_template:entries ] ;
    sh:targetClass biocore_data_model_linkml_template:PlaceholderCollection .

biocore_data_model_linkml_template:Placeholder a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents a Placeholder" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path schema:description ],
        [ sh:description "The main email address of a person" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:email ;
            sh:pattern "^\\S+@[\\S+\\.]+\\S+" ],
        [ sh:description "living or dead status" ;
            sh:in ( PATO:0001421 PATO:0001422 "UNKNOWN" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path biocore_data_model_linkml_template:vital_status ],
        [ sh:description "Number of years since birth" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path biocore_data_model_linkml_template:age_in_years ],
        [ sh:description "Date on which a person is born" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:birthDate ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:identifier ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path schema:name ] ;
    sh:targetClass biocore_data_model_linkml_template:Placeholder .

